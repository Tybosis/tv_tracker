<div class="profile-links">
  <nav>
  <%= link_to "Shows", shows_path, :class => "" %>
    <ul>
      <li><%= link_to "Browse", shows_path %></li>
      <li><%= link_to "My Shows", profile_path(@current_profile) %></li>
      <li><%= link_to "Calendar", profile_shows_path(@current_profile) %></li>
    </ul>
  </nav>

  <nav>
    <%= link_to "Settings", profiles_path, :class => "xs-text-left sm-text-center" %>
    <ul>
      <li><%= link_to "Profiles", profiles_path %></li>
      <li><%= link_to "Preferences", edit_user_registration_path %></li>
    </ul>
  </nav>
</div>

<div class="">
  <%= form_tag show_search_path, :method => 'get' do %>
    <%= text_field_tag :Search, params[:search] %>
    <%= submit_tag "Search" %>
  <% end %>
</div>


